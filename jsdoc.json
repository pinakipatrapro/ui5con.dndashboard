{
	"source": {
		"include": ["src"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs|test|localService)"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"recurse": true,
		"destination": "./docs/",
        "template": "node_modules/clean-jsdoc-theme",
		"theme_opts": {
			"default_theme":"dark"
		}
	}
}